Dump	==== nothing modified: =====================
Return	true
Dump	==== stop irda: ============================
Write	.sysconfig.irda.IRDA_MAX_BAUD_RATE "0" true
Write	.sysconfig.irda nil true
Execute	.target.bash_output "/sbin/service irda stop" $["TERM":"raw"] $["exit":0]
Read	.init.scripts.runlevel "irda" $["irda":$["start":[3]]]
Read	.init.scripts.comment "irda" $[]
Execute	.target.bash_output "/sbin/chkconfig -d irda" $["exit":0]
Return	true
Dump	==== start irda, create link: ==============
Write	.sysconfig.irda.IRDA_PORT "new_port" true
Write	.sysconfig.irda.IRDA_MAX_BAUD_RATE "0" true
Write	.sysconfig.irda nil true
Execute	.target.bash_output "/sbin/service irda stop" $["TERM":"raw"] $["exit":0]
Execute	.target.bash_output "/sbin/service irda start" $["TERM":"raw"] $["exit":0]
Read	.init.scripts.runlevel "irda" $[]
Read	.init.scripts.comment "irda" $[]
Execute	.target.bash_output "/sbin/chkconfig -a irda" $["exit":0]
Return	true
