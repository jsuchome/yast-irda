{

    // testedfiles: Irda.ycp Service.ycp
    import "Testsuite";
    import "Irda";

    map READ = $[
	"init": $[
	    "scripts": $[
		"exists"	: true
	    ],
	],
	"target": $[
	    "stat"	: $[],
	]
    ];

    Testsuite::Dump ("==== reading... ============================");

    Testsuite::Test(``(Irda::Read ()), [ READ, $[], $[] ], 0);

    Testsuite::Dump ("============================================");

    READ = $[
	"init": $[
	    "scripts": $[
		"exists"	: true
	    ],
	],
	"target": $[
	    "stat"	: $[ 1:2 ],
	],
	"sysconfig"	: $[
	    "irda"	: $[
		"IRDA_PORT"		: "/dev/ttyS1",
		"IRDA_MAX_BAUD_RATE"	: nil,
	    ],
	],
    ];

    Testsuite::Test(``(Irda::Read ()), [ READ, $[], $[] ], 0);
}

