Dump	==== start irda, create link: ==============
Write	.sysconfig.irda.IRDA_PORT "new_port" true
Write	.sysconfig.irda nil true
Read	.init.scripts.exists "irda" true
Execute	.target.bash "/etc/init.d/irda stop" $["TERM":"raw"] 0
Read	.init.scripts.exists "irda" true
Execute	.target.bash "/etc/init.d/irda start" $["TERM":"raw"] 0
Read	.init.scripts.exists "irda" true
Read	.init.scripts.exists "irda" true
Read	.init.scripts.exists "irda" true
Read	.init.scripts.runlevel "irda" $[]
Read	.init.scripts.comment "irda" $[]
Execute	.target.bash_output "/sbin/insserv -d /etc/init.d/irda" $["exit":0]
